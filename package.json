{
  "name": "goose-rarity",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool to fetch Goose setlists from elgoose.net, cache them locally, and compute rarity scores.",
  "bin": {
    "goose-rarity": "./goose_rarity.js"
  },
  "scripts": {
    "dev": "vite --config web/vite.config.ts",
    "start": "node goose_rarity.js",
    "test": "vitest run",
    "test:types": "tsgo --noEmit",
    "test:e2e": "playwright test",
    "coverage:e2e": "node scripts/convert-playwright-coverage.mjs",
    "build": "npm run web:build",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "web:dev": "vite --config web/vite.config.ts",
    "web:build": "vite build --config web/vite.config.ts",
    "web:preview": "vite preview --config web/vite.config.ts"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "papaparse": "^5.5.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "clsx": "^2.1.1",
    "idb-keyval": "^6.2.2",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
	"devDependencies": {
		"@biomejs/biome": "^2.2.7",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
		"@typescript/native-preview": "^7.0.0-dev.20251022.1",
		"@vitest/coverage-v8": "^4.0.1",
    "autoprefixer": "^10.4.21",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.2.0",
    "postcss": "^8.5.6",
		"tailwindcss": "^4.1.15",
    "vite": "^7.1.11",
    "v8-to-istanbul": "^9.3.0",
		"vitest": "^4.0.1"
	}
}
